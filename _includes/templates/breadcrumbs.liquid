			<section id="blogArchive">      
				<div class="container">
					<div class="row">
						<div class="blog-breadcrumbs-area">
							<div class="blog-breadcrumbs-left">
								<h1 class="header_2">{% if page.category %}{{ page.category }}: {% endif %}{{ site.title }}</h1>
							</div>
							<div class="blog-breadcrumbs-right">
								<ol class="breadcrumb">
									<li>You are here</li>
									<li><a href="/">Home</a></li>{% if page.layout == "method" or page.layout == "pages" %}
									<li><a href="/concept">Concept</a></li>{% endif %}                  
									{% if page.layout == "method" %}<li><a href="{{ page.url }}">{{ page.title }}{% elsif page.layout == "pages" %}<li><a href="{{ page.url | replace:page.title,'' }}">{{ page.category }}</a></li><li><a href="{{ page.url }}">{{ page.title }}{% else %}<li><a href="{{ page.url }}">{{ page.layout }}{% endif %}</a></li>
									<li class="active"><a href="{% github_edit_link %}">Source Link</a></li>
								</ol>
							</div>
						</div>
					</div>
				</div>      
			</section>
